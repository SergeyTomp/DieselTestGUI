<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="settingsUisGridPane" maxHeight="300.0" prefHeight="280.0" prefWidth="506.0" stylesheets="@../../../css/Styling.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fi.stardex.sisu.ui.controllers.uis.UisSettingsController">

  <columnConstraints>
    <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" maxWidth="200.0" minWidth="10.0" prefWidth="180.0" />
    <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="462.0" minWidth="180.0" prefWidth="180.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label fx:id="pressureSensorLabel" stylesheets="@../../../css/Styling.css" text="%settings.pressureSensor.Label">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></Label>
      <Label fx:id="languagesLabel" prefHeight="16.0" stylesheets="@../../../css/Styling.css" text="%settings.languages.ComboBox" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></Label>
      <Label fx:id="sensorAngleLabel" text="Sensor Angle" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></Label>
      <Label fx:id="rpmSourceLabel" text="RPM Source" GridPane.rowIndex="3">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></Label>
      <Spinner fx:id="sensorAngleSpinner" prefWidth="170.0" stylesheets="@../../../css/Styling.css" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </Spinner>
      <CheckBox fx:id="instantFlowCheckBox" mnemonicParsing="false" selected="true" styleClass="checkBox" stylesheets="@../../../css/Styling.css" text="%h4.settings.label.flowVisible" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <Button fx:id="firmwareButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="%settings.firmware.Button" GridPane.halignment="CENTER" GridPane.rowIndex="5" />
      <ComboBox fx:id="pressureSensorComboBox" prefWidth="170.0" GridPane.columnIndex="1" />
      <ComboBox fx:id="languagesComboBox" prefWidth="170.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <ComboBox fx:id="rpmSourceComboBox" prefWidth="170.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <ComboBox fx:id="rangeViewComboBox" prefWidth="170.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Label fx:id="rangeViewLabel" text="Flow range view" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin>
      </Label>
   </children>
</GridPane>
