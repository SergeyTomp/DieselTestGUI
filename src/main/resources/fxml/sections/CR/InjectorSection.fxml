<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>

<HBox alignment="CENTER" fillHeight="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
      minHeight="130.0" minWidth="500.0" pickOnBounds="true" xmlns="http://javafx.com/javafx/9"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="fi.stardex.sisu.ui.controllers.cr.InjectorSectionController">
    <children>
        <GridPane alignment="CENTER" gridLinesVisible="false" maxHeight="1.7976931348623157E308"
                  maxWidth="1.7976931348623157E308" minHeight="130.0" minWidth="150.0" prefHeight="130.0"
                  prefWidth="150.0" HBox.hgrow="ALWAYS">
            <children>
                <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="20.0"
                       minWidth="50.0" text="%injSection.label.width" GridPane.columnIndex="0"
                       GridPane.halignment="CENTER" GridPane.rowIndex="0"/>
                <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="20.0"
                       minWidth="50.0" text="%injSection.label.freq" GridPane.columnIndex="0"
                       GridPane.halignment="CENTER" GridPane.rowIndex="1"/>
                <Spinner fx:id="widthCurrentSignal" editable="true" maxHeight="30.0" maxWidth="90.0" minHeight="30.0"
                         minWidth="90.0" prefHeight="30.0" prefWidth="90.0" GridPane.columnIndex="1"/>
                <Spinner fx:id="freqCurrentSignal" editable="true" maxHeight="30.0" maxWidth="90.0" minHeight="30.0"
                         minWidth="90.0" prefHeight="30.0" prefWidth="90.0" GridPane.columnIndex="1"
                         GridPane.rowIndex="1"/>
            </children>
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="50.0"
                                   prefWidth="50.0"/>
                <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="100.0"
                                   prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <GridPane alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  minHeight="130.0" minWidth="50.0" prefHeight="130.0" prefWidth="50.0" HBox.hgrow="ALWAYS">
            <children>
                <RadioButton fx:id="piezoRadioButton" minHeight="-Infinity" minWidth="50.0" mnemonicParsing="false"
                             prefWidth="50.0" text="%injSection.radio.piezo" GridPane.columnIndex="0"
                             GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0"
                             GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                    <GridPane.margin>
                        <Insets bottom="2.0" left="5.0" right="2.0" top="2.0"/>
                    </GridPane.margin>
                    <toggleGroup>
                        <ToggleGroup fx:id="piezoCoilToggleGroup"/>
                    </toggleGroup>
                </RadioButton>
                <RadioButton fx:id="coilRadioButton" minHeight="-Infinity" minWidth="50.0" mnemonicParsing="false"
                             prefWidth="50.0" selected="true" text="%injSection.radio.coil"
                             toggleGroup="$piezoCoilToggleGroup" GridPane.columnIndex="0" GridPane.halignment="CENTER"
                             GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER"
                             GridPane.vgrow="ALWAYS">
                    <GridPane.margin>
                        <Insets bottom="2.0" left="5.0" right="2.0" top="2.0"/>
                    </GridPane.margin>
                </RadioButton>
                <RadioButton fx:id="piezoDelphiRadioButton" disable="true" minHeight="-Infinity" minWidth="50.0"
                             mnemonicParsing="false" prefWidth="50.0" text="%injSection.radio.piezoDelphi"
                             toggleGroup="$piezoCoilToggleGroup" GridPane.columnIndex="0" GridPane.halignment="CENTER"
                             GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" GridPane.valignment="CENTER"
                             GridPane.vgrow="ALWAYS">
                    <GridPane.margin>
                        <Insets bottom="2.0" left="5.0" right="2.0" top="2.0"/>
                    </GridPane.margin>
                </RadioButton>
            </children>
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="1.7976931348623157E308"
                                   minWidth="50.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                <RowConstraints/>
            </rowConstraints>
        </GridPane>
        <GridPane fx:id="gridLedBeaker" alignment="CENTER" maxHeight="1.7976931348623157E308"
                  maxWidth="1.7976931348623157E308" minHeight="130.0" minWidth="300.0" prefHeight="130.0"
                  prefWidth="300.0" HBox.hgrow="ALWAYS">
            <children>
                <ToggleButton fx:id="powerSwitch" minHeight="90.0" minWidth="50.0" mnemonicParsing="false"
                              prefWidth="50.0" GridPane.columnIndex="4" GridPane.halignment="RIGHT"
                              GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER"
                              GridPane.vgrow="ALWAYS">
                    <GridPane.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                    </GridPane.margin>
                </ToggleButton>
                <Label fx:id="statusBoostULabelText" alignment="TOP_RIGHT" text="%injSection.label.adjustingBoostU"
                       visible="false" GridPane.columnSpan="4" GridPane.halignment="RIGHT" GridPane.valignment="TOP"/>
                <StackPane prefHeight="150.0" prefWidth="67.0" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                           GridPane.rowIndex="1" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                    <fx:include fx:id="ledBeaker1" source="../Additional/LedBeaker.fxml"/>
                </StackPane>
                <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                           GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER"
                           GridPane.vgrow="ALWAYS">
                    <fx:include fx:id="ledBeaker2" source="../Additional/LedBeaker.fxml"/>
                </StackPane>
                <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.halignment="CENTER"
                           GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER"
                           GridPane.vgrow="ALWAYS">
                    <fx:include fx:id="ledBeaker3" source="../Additional/LedBeaker.fxml"/>
                </StackPane>
                <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.halignment="CENTER"
                           GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER"
                           GridPane.vgrow="ALWAYS">
                    <fx:include fx:id="ledBeaker4" source="../Additional/LedBeaker.fxml"/>
                </StackPane>
                <ProgressBar fx:id="switcherProgressBar" prefHeight="22.0" prefWidth="423.0" progress="0.0"
                             visible="false" GridPane.columnSpan="6" GridPane.rowIndex="2"
                             GridPane.valignment="BOTTOM"/>
            </children>
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="60.0"
                                   prefWidth="60.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="60.0"
                                   prefWidth="60.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="60.0"
                                   prefWidth="60.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="60.0"
                                   prefWidth="60.0"/>
                <ColumnConstraints fillWidth="false" halignment="RIGHT" hgrow="ALWAYS" maxWidth="1.7976931348623157E308"
                                   minWidth="50.0" prefWidth="50.0"/>
                <ColumnConstraints/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="9.0" minHeight="0.0" prefHeight="0.0"/>
                <RowConstraints maxHeight="112.0" minHeight="0.0" prefHeight="112.0"/>
                <RowConstraints maxHeight="112.0" minHeight="0.0" prefHeight="0.0"/>
                <RowConstraints maxHeight="16.0" minHeight="0.0" prefHeight="0.0"/>
            </rowConstraints>
        </GridPane>
    </children>
</HBox>
